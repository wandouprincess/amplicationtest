datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model UserSettings {
  createdAt     DateTime @default(now())
  id            String   @id @default(cuid())
  kidsAge       Int?
  kidsInterests String?
  updatedAt     DateTime @updatedAt
  userId        String?
}
