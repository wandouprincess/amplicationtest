datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Subscription {
  createdAt        DateTime                @default(now())
  endDate          DateTime?
  id               String                  @id @default(cuid())
  paymentType      String?
  startDate        DateTime?
  status           EnumSubscriptionStatus?
  subscriptionType String?
  updatedAt        DateTime                @updatedAt
  userId           String?
}

enum EnumSubscriptionStatus {
  Option1
}
