datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  authorId    String?
  content     String?
  createdAt   DateTime  @default(now())
  id          String    @id @default(cuid())
  postContent String?
  publishedAt DateTime?
  updatedAt   DateTime  @updatedAt
  userId      String?
}
