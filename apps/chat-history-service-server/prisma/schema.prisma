datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ChatHistory {
  createdAt DateTime  @default(now())
  id        String    @id @default(cuid())
  message   String?
  timestamp DateTime?
  updatedAt DateTime  @updatedAt
  userId    String?
}
